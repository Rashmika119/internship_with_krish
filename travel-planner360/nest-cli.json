{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "monorepo": true,
  "root": "apps",
  "compilerOptions": {
    "deleteOutDir": true
  },
  "projects": {
    "aggregator": {
      "type": "application",
      "root": "apps/aggregator",
      "entryFile": "main",
      "sourceRoot": "apps/aggregator/src",
      "compilerOptions": {
        "tsConfigPath": "apps/aggregator/tsconfig.app.json"
      }
    },
    "branch": {
      "type": "application",
      "root": "apps/branch",
      "entryFile": "main",
      "sourceRoot": "apps/branch/src",
      "compilerOptions": {
        "tsConfigPath": "apps/branch/tsconfig.app.json"
      }
    },
    "chain": {
      "type": "application",
      "root": "apps/chain",
      "entryFile": "main",
      "sourceRoot": "apps/chain/src",
      "compilerOptions": {
        "tsConfigPath": "apps/chain/tsconfig.app.json"
      }
    },
    "event": {
      "type": "application",
      "root": "apps/event",
      "entryFile": "main",
      "sourceRoot": "apps/event/src",
      "compilerOptions": {
        "tsConfigPath": "apps/event/tsconfig.app.json"
      }
    },
    "flight": {
      "type": "application",
      "root": "apps/flight",
      "entryFile": "main",
      "sourceRoot": "apps/flight/src",
      "compilerOptions": {
        "tsConfigPath": "apps/flight/tsconfig.app.json"
      }
    },
    "hotel": {
      "type": "application",
      "root": "apps/hotel",
      "entryFile": "main",
      "sourceRoot": "apps/hotel/src",
      "compilerOptions": {
        "tsConfigPath": "apps/hotel/tsconfig.app.json"
      }
    },
    "metrics": {
      "type": "application",
      "root": "apps/metrics",
      "entryFile": "main",
      "sourceRoot": "apps/metrics/src",
      "compilerOptions": {
        "tsConfigPath": "apps/metrics/tsconfig.app.json"
      }
    },
    "scatter": {
      "type": "application",
      "root": "apps/scatter",
      "entryFile": "main",
      "sourceRoot": "apps/scatter/src",
      "compilerOptions": {
        "tsConfigPath": "apps/scatter/tsconfig.app.json"
      }
    },
    "weather": {
      "type": "application",
      "root": "apps/weather",
      "entryFile": "main",
      "sourceRoot": "apps/weather/src",
      "compilerOptions": {
        "tsConfigPath": "apps/weather/tsconfig.app.json"
      }
    }
  }
}